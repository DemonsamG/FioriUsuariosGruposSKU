<mvc:View
	controllerName="fioriusuariosgrupossku.controller.Main"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page id="page" title="{i18n>title}">
		<content>
			<List
				id="navigationList"
				headerText="Vistas"
				items="{/navigation}">
				<StandardListItem
					title="{title}"
					type="Navigation"
					press=".onNavItemPress"/>
			</List>
		</content>
	</Page>
</mvc:View>
<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form">
	<Dialog
		id="editGroupDialog"
		title="Editar Grupo de Rol">
		<content>
			<f:SimpleForm
				editable="true"
				layout="ResponsiveGridLayout">
				
				<Label text="ID Rol"/>
				<Input value="{ROLEID}" editable="false"/>
				
				<Label text="ID Sociedad"/>
				<Input value="{IDSOCIEDAD}" editable="false"/>
				
				<Label text="ID Cedi"/>
				<Input value="{IDCEDI}" editable="false"/>

				<Label text="ID Grupo ET"/>
				<Input value="{IDGRUPOET}" editable="false"/>

				<Label text="ID"/>
				<Input value="{ID}" editable="false"/>

				<Label text="ID Privilegio"/>
				<Input value="{PRIVILEGEID}" editable="false"/>
				
				<Label text="Activo"/>
				<Switch state="{ACTIVED}"/>

			</f:SimpleForm>
		</content>
		<beginButton>
			<Button
				text="Guardar"
				type="Emphasized"
				press=".onSaveEdit"/>
		</Button>
		<endButton>
			<Button
				text="Cancelar"
				press=".onCloseEditDialog"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>